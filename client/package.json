{
  "name": "web_projectitmo2022",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.2.3",
    "emailjs-com": "^3.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "reactstrap": "^9.1.5",
    "swiper": "^8.4.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "test:jest": "react-scripts test --watchAll=false --bail",
    "test:cypress": "start-server-and-test start http://localhost:3000 cypress",
    "cypress": "cypress run",
    "eject": "react-scripts eject",
    "eslint": "npx eslint 'src/**/*.js' 'src/**/**/*.js'",
    "prettier": "npx prettier'src/**/*.js' 'src/**/**/*.js'",
    "stylelint": "stylelint 'src/**/*.css' 'src/**/**/*.css'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "cd ../ && husky install client/.husky"
  },
  "proxy": "http://localhost:5000",
  "lint-staged": {
    "./src/**/*.js": [
      "eslint",
      "prettier"
    ],
    "./src/**/**/*.js": [
      "eslint",
      "prettier"
    ],
    "./src/**/*.css": [
      "stylelint"
    ],
    "./src/**/**/*.css": [
      "stylelint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cypress": "^12.2.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react-ga": "^3.3.1",
    "redux": "^4.2.0",
    "start-server-and-test": "^1.15.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "textlint": "^12.2.4",
    "textlint-rule-no-todo": "^2.0.1"
  }
}
